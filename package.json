{
  "name": "vscode-translator",
  "displayName": "vscode-translator",
  "description": "a vscode translator",
  "version": "0.1.1",
  "publisher": "chenng",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.vscodeTranslate"
  ],
  "main": "./out/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.vscodeTranslate",
        "key": "meta+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "extension.vscodeTranslate",
        "title": "VSCode Translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "translation.js": "^0.7.5"
  },
  "repository": "https://github.com/ringcrl/vscode-translator"
}
